<Window x:Class="Timber.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Timber"
        mc:Ignorable="d"
        Title="LoginWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="usernameBox" HorizontalAlignment="Left" Height="23" Margin="280,150,0,0" TextWrapping="Wrap" Text="admin" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="passwordBox" HorizontalAlignment="Left" Height="23" Margin="280,178,0,0" TextWrapping="Wrap" Text="admin" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Log in" HorizontalAlignment="Left" Margin="303,221,0,0" VerticalAlignment="Top" Width="75" Click="LoginButtonClick"/>
        <Button x:Name="button1" Content="Create Account" HorizontalAlignment="Left" Margin="567,349,0,0" VerticalAlignment="Top" Width="107"/>

    </Grid>
</Window>
